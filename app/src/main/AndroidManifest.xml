<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.collegeevent">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/edit2"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/edit2"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat">
        <activity android:name=".viewResult"></activity>
        <activity android:name=".showParticipantList" />
        <activity android:name=".EventActions" />
        <activity android:name=".registerStudent" />
        <activity android:name=".create_event" />
        <activity android:name=".eventView" />
        <activity android:name=".registerActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>